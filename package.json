{
  "name": "reaux-dom-demo-project",
  "private": true,
  "frame": "reaux-dom",
  "scripts": {
    "reinstall": "rm -rf node_modules && rm -rf yarn.lock && yarn install",
    "dev": "node config/start.js",
    "build": "yarn prettier && node config/build.js",
    "build:analyze": "yarn prettier && ANALYZE=true node config/build.js",
    "type-check": "tsc --pretty --noEmit",
    "eslint": "eslint . --fix",
    "stylelint": "stylelint '**/*.scss'",
    "prettier": "prettier --write .",
    "api": "node config/api.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.@(tsx|ts)": [
      "yarn eslint",
      "yarn prettier",
      "git add"
    ],
    "*.@(js|json|md|css)": [
      "yarn prettier",
      "git add"
    ],
    "*.@(scss|css)": [
      "yarn stylelint",
      "yarn prettier",
      "git add"
    ]
  },
  "dependencies": {
    "@fingerprintjs/fingerprintjs": "^3.4.1",
    "antd": "^5.9.3",
    "axios": "^1.3.3",
    "blueimp-md5": "^2.18.0",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "normalize.css": "^8.0.1",
    "reaux-dom": "1.3.1",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@babel/plugin-transform-runtime": "^7.21.0",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.10",
    "@types/blueimp-md5": "^2.18.0",
    "@types/js-cookie": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "babel-loader": "^9.0.0",
    "babel-plugin-import": "^1.13.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^1.0.1",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "html-webpack-plugin": "^4.5.2",
    "husky": "4.3.8",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "lint-staged": "^12.3.4",
    "mini-css-extract-plugin": "^2.7.2",
    "postcss-loader": "^7.0.2",
    "postcss-preset-env": "^8.4.1",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.5.1",
    "react-refresh": "^0.14.0",
    "sass": "^1.49.8",
    "sass-loader": "^13.1.0",
    "stylelint": "^14.7.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "terser-webpack-plugin": "^5.3.6",
    "typescript": "^4.5.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-dev-server": "^4.11.1",
    "yapi-ts-engine": "^0.1.1"
  },
  "resolutions": {
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13"
  }
}
