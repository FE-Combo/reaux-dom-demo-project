{
  "name": "reaux-dom-demo-project",
  "private": true,
  "frame": "reaux-dom",
  "scripts": {
    "reinstall": "rm -rf node_modules && rm -rf yarn.lock && yarn install",
    "dev": "rs dev",
    "build": "rs build",
    "build:analyze": "ANALYZE=true yarn build",
    "type-check": "tsc --pretty --noEmit",
    "eslint": "eslint . --fix",
    "stylelint": "stylelint '**/*.scss'",
    "prettier": "prettier --write ./src",
    "api": "node config/api.js",
    "eject": "rs eject"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.@(tsx|ts)": [
      "yarn eslint",
      "yarn prettier",
      "git add"
    ],
    "*.@(js|json|md|css)": [
      "yarn prettier",
      "git add"
    ],
    "*.@(scss|css)": [
      "yarn stylelint",
      "yarn prettier",
      "git add"
    ]
  },
  "dependencies": {
    "@fingerprintjs/fingerprintjs": "^3.4.1",
    "antd": "^5.9.3",
    "axios": "^1.3.3",
    "blueimp-md5": "^2.18.0",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "normalize.css": "^8.0.1",
    "reaux-dom": "1.3.1",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "typescript": "^4.5.4",
    "@types/blueimp-md5": "^2.18.0",
    "@types/js-cookie": "^3.0.1",
    "yapi-ts-engine": "^0.1.1",
    "reaux-scripts": "^0.2.1"
  },
  "resolutions": {
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13"
  },
  "devServerOptions": {
    "proxy": [
      {
        "context": [
          "/api"
        ],
        "target": "http://192.168.1.1:8080",
        "secure": false,
        "pathRewrite": {
          "^/api": ""
        },
        "changeOrigin": true
      }
    ]
  }
}
